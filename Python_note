num1=int(input("숫자1 입력:")) #입력반은 숫자 정수로 변환(int)
num2=int(input("숫자2 입력:"))
num3=float(input("숫자3 입력:")) #입력반은 숫자 실수로 변환(int)

print("num1+num2=", num1+num2)
print("num1+num2+num3=", num1+num2+num3)

